- if has_any_label?
  = part(:label_wrapper) do
    - if has_start_label?
      - if start?
        = start
      - else
        = part(:start) do
          = @start

    = part(:component)

    - if has_end_label?
      - if end?
        = self.send(:end)
      - else
        = part(:end) do
          = @end

- else
  = part(:component)
